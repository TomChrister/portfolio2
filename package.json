{
  "name": "react-tailwind-portfolio",
  "description": "An open-source portfolio template built with React and Tailwind.",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "aos": "^2.3.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-github-btn": "^1.2.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.5"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run watch:css && react-scripts build",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss-cli": "^8.1.0"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  }
}
